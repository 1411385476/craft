}
template template2{
	parser('@',$MSG,$MSG1,$MSG2);
	parser(' ',$MSG2,$MSG3,$MSG4);
	sql("insert into config(`cid`,`datetime`,`host`,`priority`,`facility`, \
	`user`,`module`,`content`) values('',$datetime,$host,$PRI,$FAC,$MSG1,$MSG3,$MSG4);");
}
